{
  "name": "save session",
  "version": "1.0",
  "description": "saves your session",
  "permissions": ["tabs", "bookmarks"],
  "browser_action": {
    "default_title": "Save Session",
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "manifest_version": 2
}
